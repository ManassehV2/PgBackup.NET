<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>PgBackup.Net</PackageId>
    <Version>1.0.0</Version>
    <Authors>Minase Mekete</Authors>
    <Description>a simple library to backup postgres db on demand</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>pg_dump, pg_restore, Postgresql backup</PackageTags>
    <RepositoryURL>https://github.com/ManassehV2/PgBackup.NET.git</RepositoryURL>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
  </ItemGroup>

</Project>
